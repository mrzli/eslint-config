{
  "name": "@gmjs/eslint-config",
  "version": "0.0.12",
  "description": "eslint-config",
  "author": {
    "name": "Goran Mr≈æljak",
    "email": "goran.mrzljak@gmail.com",
    "url": "https://mrzli.com"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-configuration",
    "linter",
    "linting",
    "javascript-linter",
    "code-quality",
    "static-analysis",
    "style-guide",
    "coding-style"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzli/eslint-config"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "prettier": "@gmjs/prettier-config",
  "scripts": {
    "start:dev": "ts-node src/index.ts",
    "lint": "eslint --fix . && prettier --write .",
    "lint:nofix": "eslint . && prettier .",
    "test-only": "echo \"test\"",
    "test": "npm run lint && npm run test-only",
    "build-only": "shx rm -rf ./dist && tsc --project tsconfig.lib.json",
    "build": "npm run test && npm run build-only",
    "pub-only": "npm pub --access public",
    "pub": "npm run build && npm run pub-only"
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@gmjs/prettier-config": "^1.0.2",
    "@types/eslint": "^8.40.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  }
}
